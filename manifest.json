{
    "manifest_version": 3,
    "name": "UserStyle Loader",
    "version": "0.1",
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": "<all_urls>",
            "js": []
        }
    ],
    "default_locale": "en",
    "permissions": ["activeTab"]
  }